<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" metadata-complete="true" version="3.0">
  <description>
      GPS Enabled Servlets
    </description>
  <display-name>Servlets</display-name>
  <servlet>
    <servlet-name>a1</servlet-name>
    <servlet-class>com.comm.StoreServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>a1</servlet-name>
    <url-pattern>/store</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>a2</servlet-name>
    <servlet-class>com.comm.RollServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>a2</servlet-name>
    <url-pattern>/roll</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>a3</servlet-name>
    <servlet-class>com.comm.AttendServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>a3</servlet-name>
    <url-pattern>/attend</url-pattern>
  </servlet-mapping>
</web-app>